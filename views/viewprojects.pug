extends layouts/dashboad

block content
  doctype html
  head
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    title Student Project Information
    style.
      body {
        font-family: Arial, sans-serif;
        background-color: #e6f7ff;
      }
      h2{
        text-align:center;
      }
      table {
        width: 60%;
        border-collapse: collapse;
        text-align: left;
        margin: auto;
        background-color: #e1e2e3;
      }

  h2 Student Information
  table(border='1')
    thead
      tr
        th Name of Student
        th Registration Number
        th Email
        th Project Title
        th Project Documentation
        //- th Name of Supervisor
    tbody#studentData
      each student in studentData
        tr
          td= student.first_name + ' ' + student.middle_name + ' ' +student.surname
          td= student.registration_no
          td= student.email
          td= student.tittle
          td= student.documentation
          //- td= student.superviser_id
